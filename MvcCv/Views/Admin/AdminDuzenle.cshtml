@model MvcCv.Models.Entity.TblAdmin

@{
    ViewBag.Title = "AdminDuzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<h1>Deneyim Guncelleme Sayfasi</h1>
<br />
<form class="form-group" method="post">
    @Html.Label("Admin ID")
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    <br />

    @Html.Label("Kullanici Adi")
    @Html.TextBoxFor(x => x.KullaniciAdi, new { @class = "form-control" })
    <br />

    @Html.Label("Sifre")
    @Html.TextBoxFor(x => x.Sifre, new { @class = "form-control" })
    <br />

   

    <button class="btn btn-info">Guncelle</button>
</form>



