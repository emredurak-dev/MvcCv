@model MvcCv.Models.Entity.TblYetenekler

@{
    ViewBag.Title = "YetenekDuzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h1>Yetenek Duzenleme Sayfasi</h1>
<br />
<form class="form-group" method="post">


    @Html.Label("Yetenek Aciklamasi")
    @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
    <br />

    @Html.Label("Yetenek Orani")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })
    <br />

    <button class="btn btn-info">Guncelle</button>
</form>
