@model MvcCv.Models.Entity.TblSosyalMedya

@{
    ViewBag.Title = "SayfaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<h1>Sosyal Medya Hesap Duzenleme Sayfasi</h1>
<br />
<form class="form-group" method="post">


    @Html.Label("Platform Adi")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />

    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />

    @Html.Label("Icon Degeri")
    @Html.TextBoxFor(x => x.icon, new { @class = "form-control" })
    <br />

    <button class="btn btn-info">Guncelle</button>
</form>

